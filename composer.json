{
  "name": "supergrecko/riot-quest",
  "description": "Riot Games API Framework",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "RiotQuest\\": "src/RiotQuest/"
    }
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "6.3.3",
    "symfony/dotenv": "4.2.0",
    "nesbot/carbon": "2.9.1",
    "psr/simple-cache": "1.0.1",
    "league/flysystem": "^1.0",
    "illuminate/support": "5.8.*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5",
    "symfony/var-dumper": "4.2.0",
    "jenssegers/blade": "1.2.1"
  },
  "prefer-stable": true,
  "minimum-stability": "RC",
  "extra": {
    "laravel": {
      "providers": [
        "RiotQuest\\ServiceProviders\\LeagueServiceProvider"
      ]
    }
  }
}
